{
	"name": "Chrome Extension Manager",
	"version": "1.0",
	"description": "Extension to Manage other extensions",
	"permissions": ["management", "storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"icons": { "18": "icon18.png", "48": "icon48.png", "96": "icon96.png", "128": "icon128.png" },
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"manifest_version": 2
}
